<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        
        
        <record id="view_custom_report_res_company_form" model="ir.ui.view">
            <field name="name">view_custom_report_res_company_form</field>
            <field name="model">res.company</field>
            <field name="priority" eval="101" />
            <field name="inherit_id" ref="base.view_company_form" />
            <field name="arch" type="xml">

                <xpath expr="//field[@name='enable_bo_edi']" position="after">
                    <field name="enable_footnote" widget="boolean_toggle" invisible='not enable_bo_edi'/>
                </xpath>

            </field>
        </record>
    </data>
</odoo>
